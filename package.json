{
  "name": "lujs",
  "version": "0.1.0",
  "description": "A simple LEGO Universe server written in Node",
  "main": "index.ts",
  "bin": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier-standard --format --lint",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "linters": {
      "**/*": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RaineBannister/LUJS"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "extract-zip": "^2.0.1",
    "fs-extra": "^8.1.0",
    "inet-aton": "git+https://git@github.com/RaineBannister/inet-aton.git",
    "lugamemessages": "git+https://git@github.com/RaineBannister/lu-game-messages.git",
    "md5-file": "^4.0.0",
    "node-raknet": "git+https://git@github.com/RaineBannister/node-raknet.git",
    "sequelize": "^5.22.3",
    "sqlite": "^3.0.6",
    "sqlite3": "^4.2.0",
    "typescript": "^3.9.7",
    "xmlbuilder": "^13.0.2"
  },
  "author": "Raine Bannister",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RaineBannister/lujs/issues"
  },
  "homepage": "https://github.com/RaineBannister/lujs#readme",
  "pkg": {
    "scripts": "Handles/**/*.js",
    "assets": "res/*"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "pkg": "^4.4.9",
    "prettier-standard": "^16.4.1"
  }
}
